             xmlns:local="clr-namespace:Delfinovin"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources\Themes\Fluent.xaml"/>
